<?xml version="1.0"?>
<robot name="myfirst">

  <link name="world"/>
  <!-- Link 1 -->
  <link name="base_link">
    <visual>
      <geometry>
        <cylinder length="0.6" radius="0.2"/>
        <!-- tesst -->
      </geometry>
      <origin rpy="0 0 0" xyz="0 0 0.3"/>
    </visual>
    <collision>
      <geometry>
          <cylinder radius="0.3" length="0.6" />
      </geometry>
      <origin xyz="0 0 -0.3" rpy="0 0 0"/>
    </collision>
  </link>

  <joint name="world_to_base" type="fixed">
    <parent link="world"/>
    <child link="base_link"/>
  </joint>


</robot>
